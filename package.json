    "pako": "^2.1.0",
    "pinia": "2.3.0",
    "vue": "^3.3.4",
    "vue-demi": "0.14.10",
    "vue-dompurify-html": "^5.1.0",
    "vue-router": "^4.2.4",
    "vuetify": "^3.7.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/js-cookie": "^3.0.3",
