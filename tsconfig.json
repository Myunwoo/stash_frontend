{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "types": [
      "node"
    ]
  },
  "exclude": [
    "node_modules",
    ".output"
  ]
}